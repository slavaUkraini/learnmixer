<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hcilearnmix.learnmixer"
    android:versionCode="1"
    android:versionName="1.0" >

  

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <!--<activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>-->
        <activity

            android:name="com.hcilearnmix.learnmixer.DashBoardActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         
         <activity

            android:name="com.hcilearnmix.learnmixer.MyCollection"
            android:label="@string/app_name" />
      
         <activity
            android:name="com.hcilearnmix.learnmixer.Share_Activity"
            android:label="Friends"
            android:parentActivityName=".MyCollection" >
        </activity>
         <activity
            android:name="com.hcilearnmix.learnmixer.MainActivity"
            android:parentActivityName=".MyCollection" >
        </activity>
        <activity
            android:name="com.hcilearnmix.learnmixer.pdf_activity"
            android:label="PDF"
            android:parentActivityName=".MyCollection" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.hcilearnmix.learnmixer.MyCollection" />
        </activity>
        <activity
            android:name="com.hcilearnmix.learnmixer.VideoActivity"
            android:label="Video"
            android:parentActivityName=".MyCollection">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.hcilearnmix.learnmixer.MyCollection" />
        </activity>

    </application>

</manifest>
